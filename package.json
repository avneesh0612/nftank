    "typescript": "4.7.2"
  },
  "oclif": {
    "bin": "nftank",
    "dirname": "nftank",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topics": {
      "mint": {
        "description": "âœ¨ Mint an NFT to a given address"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
