    "prepack": "yarn build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/buidler-hub/nftank/issues",
